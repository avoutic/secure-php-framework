{
    "name": "avoutic/web-framework",
    "description": "web-framework is a secure-by-default PHP framework for quickly making PHP sites and APIs",
    "type": "library",
    "keywords": [
        "framework",
        "security"
    ],
    "homepage": "https://avoutic.github.io/web-framework/",
    "license": "MIT",
    "authors": [
        {
            "name": "Avoutic",
            "email": "avoutic@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/avoutic/web-framework/issues",
        "source": "https://github.com/avoutic/web-framework",
        "docs": "https://avoutic.github.io/web-framework/"
    },
    "require": {
        "php": "^8.2",
        "ext-mysqli": "*",
        "wildbit/postmark-php": "^4.0",
        "stripe/stripe-php": "^7.128",
        "cache/redis-adapter": "^1.1",
        "latte/latte": "^3.0",
        "ext-redis": "*",
        "slim/psr7": "^1.6",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.1",
        "php-di/php-di": "^7.0",
        "slim/slim": "^4.11"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/verify": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "WebFramework\\Core\\": "includes/",
            "WebFramework\\Exception\\": "includes/Exception/",
            "WebFramework\\Middleware\\": "includes/Middleware/",
            "WebFramework\\Security\\": "includes/Security/",
            "WebFramework\\Actions\\": "actions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebFramework\\PHPStan\\": "phpstan/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": false
        }
    }
}
